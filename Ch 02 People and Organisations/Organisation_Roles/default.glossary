   <glossaryWord name="Internal Organisation" plural="Internal Organisations" abbreviation="int org" alt_abbreviation="" primary_word="true" id="55AF3780-C1D8-35FE-CF7C-CC5DA9C7724B" import_id="" description="An organisation that is part of the enterprise for whom the data model is developed. " />

