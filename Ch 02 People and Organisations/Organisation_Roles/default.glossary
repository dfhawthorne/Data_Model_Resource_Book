   <glossaryWord name="Distribution Channel" plural="Distribution Channels" abbreviation="dist chan" alt_abbreviation="" primary_word="true" id="E1E39BDA-0C44-F46A-6835-DF33B075CEFB" import_id="" description="An organisation that markets the enterprise&apos;s products" />
   <glossaryWord name="Agent" plural="Agents" abbreviation="agt" alt_abbreviation="" primary_word="true" id="5267FAA6-29F7-E6BA-898D-BC6186B12ABE" import_id="" description="markets these products without buying or carrying goods" />
